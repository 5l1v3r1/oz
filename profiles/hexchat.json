{
"path": "/usr/bin/hexchat"
, "xserver": {
	"enabled": true
	, "enable_tray": false
	, "tray_icon": "/usr/share/icons/hicolor/scalable/apps/hexchat.svg"
	, "enable_notifications": true
}
, "networking":{
	"type":"empty"
	, "sockets": [
		{"type":"client", "proto":"tcp", "port":9050}
	]
}
, "whitelist": [
	{"path":"${HOME}/.config/hexchat", "can_create":true}

	, {"path":"${HOME}/.config/gtk-3.0", "ignore":true}
	, {"path":"${HOME}/.config/gtk-2.0", "ignore":true}
	, {"path":"/var/lib/oz/cells.d/hexchat.json"}
	, {"path":"/var/lib/oz/cells.d/hexchat-whitelist.seccomp"}
]
, "blacklist": [
]
, "seccomp": {
	"mode":"whitelist"
	, "enforce": true
	, "whitelist": "/var/lib/oz/cells.d/hexchat-whitelist.seccomp"
}
}
