{
"path": "/usr/bin/icedove"
, "xserver": {
	"enabled": true
	, "enable_tray": false
	, "tray_icon": "/usr/share/icons/hicolor/scalable/apps/icedove.svg"
}
, "networking":{
	"type":"bridge"
	, "sockets": [
		{"type":"client", "proto":"tcp", "port":9050}
	]
}
, "whitelist": [
	{"path":"/run/resolvconf"}
	, {"path":"/run/user/${UID}/keyring/control", "ignore":true}
	, {"path":"${HOME}/.local/share/keyrings", "ignore":true}
	, {"path":"/tmp/gpg-*/S.gpg-agent", "ignore":true}
	, {"path":"${HOME}/.cache/icedove", "can_create":true}
	, {"path":"${HOME}/.gnupg", "ignore":true}
	, {"path":"${HOME}/.icedove", "can_create":true}

	, {"path":"${HOME}/.config/gtk-3.0", "ignore":true}
	, {"path":"${HOME}/.config/gtk-2.0", "ignore":true}
]
, "_blacklist": [
]
, "environment": [
	{"name":"GPG_AGENT_INFO"}
	, {"name":"GNOME_KEYRING_CONTROL"}
	, {"name":"GNOME_KEYRING_PID", "value":"1"}
]
, "seccomp": {
	"mode":"blacklist"
	, "enforce": true
}
}
